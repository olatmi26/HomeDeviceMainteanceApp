    UserDocument: { user_id: 'integer unsigned nullable', IdCard: 'string nullable', PassportDocument: 'string nullable', LegalPapersUploaded: 'string nullable', OtherDocsHandPrint: 'string nullable' }
    Admin: { firstName: string, lastName: string, phoneN0: 'string,11', email: 'string unique', profilePicture: longtext, password: string, relationships: { belongsToMany: \Spatie\LaravelPermission\Models\Role } }
    Category: { name: 'string nullable', description: longtext }
    Stock: { partTrackN0: 'string unique', category_id: 'id foreign nullable', partName: 'string index', UnitCost: 'unsigned integer nullable', Maker: string, ModelNo: string, DateStock: dateTime, Type: 'string nullable', QtyInstock: 'unsigned integer nullable', Availability: 'unsigned integer nullable default:1', status: 'string default:Active nullable', stockBy: 'integer unsigned nullable' }
    StockCount: { category_id: 'id foreign nullable', QtyInstock: 'integer unsigned nullable', stockUpdatedBy: 'unsigned integer nullable' }
    Customer: { FirstName: 'string:20', LastName: 'string:20', OtherName: 'string:25 nullable', PhoneNo: 'string:11 unique', PhoneNo2: 'string:11 unique nullable', ProfileImage: 'string nullable', email: 'string unique', Address: longtext, city_id: 'unsigned integer nullable', state_id: 'unsigned integer nullable', password: string, CurrentGpsLocationLong: 'decimal:10,8 nullable', CurrentGpsLocationLat: 'decimal:10,8 nullable', CustomerStatus: 'boolean default:true', email_verified_at: timestamp, relationships: { hasMany: Order } }
    Order: { customer_id: 'id foreign nullable', OrderTrackN0: 'string:6 unique', stock_id: 'id foreign nullable', DateOrders: dateTime, isOrderService: 'boolean default:false nullable', relationships: { hasMany: OrderItem } }
    OrderItem: { order_id: 'id foreign nullable', category_id: 'id foreign nullable', stock_id: 'id foreign nullable', QtyOrdered: 'integer nullable index', UnitCost: 'integer nullable', TotalCost: 'integer nullable', DateOrder: datetime, OrderServiced: 'boolean default:false' }
