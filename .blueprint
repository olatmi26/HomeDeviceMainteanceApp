    CompanyAssets\VehicleDocument: { vehicle_id: 'unsigned integer nullable', DocumentName: 'string nullable', DocumentPaper: 'string nullable' }
    CompanyAssets\VehiclePartDetail: { vehicle_id: 'unsigned integer nullable', OilType: 'string nullable', OilMeter: 'integer nullable', OilfilterGuaged: 'boolean nullable default:true', BatteryUsed: 'string  nullable', ACCondition: 'boolean nullable' }
    OilFilter: { vehicle_id: 'id foreign nullable', WhenFIlterChanged: datetime, ExpectedNextChange: datetime, ChangedBy: 'id foreign:users.id nullable' }
    PartFault: { partFualty: 'string nullable', ExactFualt: longtext, PartReplacewith: 'id foreign:stocks.id nullable', PartChanged: 'id foreign:stocks.id nullable', fualtyCOmponentPicture: 'string nullable' }
    CompanyAssets\Carservice: { vehicle_id: 'unsigned integer nullable', isCustomerCar: 'boolean nullable', customer_id: 'id foreign:customers.id nullable', isCompanyCar: 'boolean nullable', partfault_id: 'id foreign nullable', fualtyCOmponentPicture: 'string nullable', RepairCost: 'integer nullable index', TotalCost: 'integer nullable', ServiceBy: 'id foreign:users.id nullable', DateService: dateTime }
    CompanyAssets\CarFuelling: { vehicle_id: 'unsigned integer nullable', department_id: 'unsigned integer nullable', FuelLiter: 'integer nullable', UnitCost: 'unsigned integer nullable', DateFuelled: dateTime, RefilledBy: 'id foreign:users.id nullable' }
    OrderAssignTo: { order_id: 'id foreign nullable', AssignedTo: 'id foreign:users.id nullable', OrderCompleted: 'boolean default:false nullable', AssignedBy: 'id foreign:users.id', relationships: { hasMany: 'Order, CarRepairEvidence' } }
